define DEFAULT_OUTPUTS null_output

include <%= node['nxlog_ce']['conf_dir'] %>/nxlog.conf.d/op_*.default

<Input <%= @name %>>
  <% @params.each do |param| -%>
  <%= param[0] %> <%= param[1] %>
  <% end -%>
</Input>

<Route r_<%= @name %>>
  Path <%= @name %> => <%= @destinations.join(', ') %>
</Route>
